        feature: layer.feature,
        variables: {}
    };
    layer.bindTooltip((exp_label_tarajet_2_eval_expression(context) !== null?String('<div style="color: #000000; font-size: 10pt; font-family: \'MS Shell Dlg 2\', sans-serif;">' + exp_label_tarajet_2_eval_expression(context)) + '</div>':''), {permanent: true, offset: [-0, -16], className: 'css_tarajet_2'});
    labels.push(layer);
    totalMarkers += 1;
      layer.added = true;
      addLabel(layer, i);
      i++;
});
var i = 0;
layer_PLACEVISETE_3.eachLayer(function(layer) {
    var context = {
        feature: layer.feature,
        variables: {}
    };
    layer.bindTooltip((layer.feature.properties['nom'] !== null?String('<div style="color: #000000; font-size: 9pt; font-family: \'MS Shell Dlg 2\', sans-serif;">' + layer.feature.properties['nom']) + '</div>':''), {permanent: true, offset: [-0, -16], className: 'css_PLACEVISETE_3'});
    labels.push(layer);
    totalMarkers += 1;
      layer.added = true;
      addLabel(layer, i);
      i++;
});
resetLabels([layer_Aeroportouarzazate_1,layer_tarajet_2,layer_PLACEVISETE_3]);
map.on("zoomend", function(){
    resetLabels([layer_Aeroportouarzazate_1,layer_tarajet_2,layer_PLACEVISETE_3]);
});
map.on("layeradd", function(){
    resetLabels([layer_Aeroportouarzazate_1,layer_tarajet_2,layer_PLACEVISETE_3]);
});
map.on("layerremove", function(){
    resetLabels([layer_Aeroportouarzazate_1,layer_tarajet_2,layer_PLACEVISETE_3]);
});
</script>

