function pop_Aeroportouarzazate_1(feature, layer) {
    layer.on({
        mouseout: function(e) {
            for (var i in e.target._eventParents) {
                if (typeof e.target._eventParents[i].resetStyle === 'function') {
                    e.target._eventParents[i].resetStyle(e.target);
                }
            }
            if (typeof layer.closePopup == 'function') {
                layer.closePopup();
            } else {
                layer.eachLayer(function(feature){
                    feature.closePopup()
                });
            }
        },
        mouseover: highlightFeature,
    });
    var popupContent = '<table>\
            <tr>\
                <th scope="row">id</th>\
                <td>' + (feature.properties['id'] !== null ? autolinker.link(feature.properties['id'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <th scope="row">nom</th>\
                <td>' + (feature.properties['nom'] !== null ? autolinker.link(feature.properties['nom'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <th scope="row">x</th>\
                <td>' + (feature.properties['x'] !== null ? autolinker.link(feature.properties['x'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <th scope="row">y</th>\
                <td>' + (feature.properties['y'] !== null ? autolinker.link(feature.properties['y'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <th scope="row">info</th>\
                <td>' + (feature.properties['info'] !== null ? autolinker.link(feature.properties['info'].toLocaleString()) : '') + '</td>\
            </tr>\
        </table>';
    var content = removeEmptyRowsFromPopupContent(popupContent, feature);
    layer.on('popupopen', function(e) {
        addClassToPopupIfMedia(content, e.popup);
    });
    layer.bindPopup(content, { maxHeight: 400 });
}

function style_Aeroportouarzazate_1_0() {
    return {
        pane: 'pane_Aeroportouarzazate_1',
rotationAngle: 0.0,
rotationOrigin: 'center center',
icon: L.icon({
    iconUrl: 'markers/Aeroportouarzazate_1.svg',
    iconSize: [28.88, 28.88]
}),
        interactive: true,
    }
}
map.createPane('pane_Aeroportouarzazate_1');
map.getPane('pane_Aeroportouarzazate_1').style.zIndex = 401;
map.getPane('pane_Aeroportouarzazate_1').style['mix-blend-mode'] = 'normal';
var layer_Aeroportouarzazate_1 = new L.geoJson(json_Aeroportouarzazate_1, {
    attribution: '',
    interactive: true,
    dataVar: 'json_Aeroportouarzazate_1',
    layerName: 'layer_Aeroportouarzazate_1',
    pane: 'pane_Aeroportouarzazate_1',
    onEachFeature: pop_Aeroportouarzazate_1,
    pointToLayer: function (feature, latlng) {
        var context = {
            feature: feature,
            variables: {}
        };
        return L.marker(latlng, style_Aeroportouarzazate_1_0(feature));
    },
});
bounds_group.addLayer(layer_Aeroportouarzazate_1);
map.addLayer(layer_Aeroportouarzazate_1);
function pop_tarajet_2(feature, layer) {
    layer.on({
        mouseout: function(e) {
            for (var i in e.target._eventParents) {
                if (typeof e.target._eventParents[i].resetStyle === 'function') {
                    e.target._eventParents[i].resetStyle(e.target);
                }
            }
            if (typeof layer.closePopup == 'function') {
                layer.closePopup();
            } else {
                layer.eachLayer(function(feature){
                    feature.closePopup()
                });
            }
        },
        mouseover: highlightFeature,
    });
    var popupContent = '<table>\
            <tr>\
                <td colspan="2">' + (feature.properties['id'] !== null ? autolinker.link(feature.properties['id'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <td colspan="2">' + (feature.properties['long'] !== null ? autolinker.link(feature.properties['long'].toLocaleString()) : '') + '</td>\
            </tr>\
            <tr>\
                <td colspan="2">' + (feature.properties['durée'] !== null ? autolinker.link(feature.properties['durée'].toLocaleString()) : '') + '</td>\
            </tr>\
        </table>';
    var content = removeEmptyRowsFromPopupContent(popupContent, feature);
    layer.on('popupopen', function(e) {
        addClassToPopupIfMedia(content, e.popup);
    });
    layer.bindPopup(content, { maxHeight: 400 });
}

